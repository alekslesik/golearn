    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "attach",ะบั
            "name": "Docker: Attach to Node",
            "remoteRoot": "/usr/src/app"
        },
        {
            "name": "Connect to server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}",
            "port": 9999,
            "host": "127.0.0.1"
        },
        {
            "name": "Connect local",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "remotePath": "${workspaceFolder}",
            "port": 9999,
            "host": "127.0.0.1"
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        }
    ]
}
